{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Pages"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "is_private": {
      "type": "boolean",
      "default": false
    },
    "is_included": {
      "type": "boolean",
      "default": true
    },
    "order": {
      "type": "integer"
    }
  }
}
